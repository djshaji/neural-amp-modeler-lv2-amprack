unset $NDK
NDK=~/Android/Sdk/ndk/26.1.10909125
MINSDKVERSION=31
find libs/ -type f -delete
for ABI in arm64-v8a  armeabi-v7a  x86  x86_64
do
    make clean
    echo building for $ABI
    cmake --fresh    -DCMAKE_TOOLCHAIN_FILE=/home/djshaji/Android/Sdk/ndk/26.1.10909125/build/cmake/android.toolchain.cmake     -DANDROID_ABI=$ABI     -DANDROID_PLATFORM=31
    make
    mv src/neural_amp_modeler.so libs/$ABI
done
#echo $NDK
cp -rv libs/* ~/projects/amp-rack/app/src/main/jniLibs/
